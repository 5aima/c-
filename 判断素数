#include<stdio.h>
#include<math.h>
int main(void){
	int m,i,k;
	printf("Please enter a number:");
	scanf("%d",&m);
	k=(int)sqrt(m);
	for(i=2;i<=k;i++){
		if(m%i==0)
			break;
	}
	if(i>k)
		printf("yes!\n");
	else
		printf("no!\n");
	printf("Program is over!\n");
	return 0;
}
